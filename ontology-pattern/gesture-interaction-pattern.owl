    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1Affordance -->

    <owl:Class rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1Affordance">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1affordedBy"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1Device"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1supportsGesture"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1Gesture"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Affordance is a quality or characteristic of Devices that indicates the possible action a Human can take with it. This is rdfs:subClassOf ssn:Property.</rdfs:comment>
        <rdfs:label xml:lang="en">Affordance</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1BodyPart -->

    <owl:Class rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1BodyPart">
        <rdfs:comment xml:lang="en">BodyPart is any part of HumanBody such as an Face, Leg etc.</rdfs:comment>
        <rdfs:label xml:lang="en">BodyPart</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1Device -->

    <owl:Class rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1Device">
        <rdfs:comment xml:lang="en">Device denotes a piece of hardware or software which consists of sensors or actuators or both.</rdfs:comment>
        <rdfs:label xml:lang="en">Device</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1Duration -->

    <owl:Class rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1Duration">
        <rdfs:comment xml:lang="en">Duration of a temporal extent expressed as a decimal number scaled by a temporal unit. This is an owl:equivalentClass time:Duration.</rdfs:comment>
        <rdfs:label xml:lang="en">Duration</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1Gesture -->

    <owl:Class rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1Gesture">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1includesGesture"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1Gesture"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1used"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1BodyPart"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1hasEndPose"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1Pose"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1hasStartPose"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1Pose"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1hasMovement"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1Movement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Gesture is a Movement or a Pose of a BodyPart.</rdfs:comment>
        <rdfs:label xml:lang="en">Gesture</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1Human -->

    <owl:Class rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1Human">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1performs"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1Gesture"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1performs"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1Pose"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1uses"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1BodyPart"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Human represents the user who performs a Gesture or a Pose.</rdfs:comment>
        <rdfs:label xml:lang="en">Human</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1Movement -->

    <owl:Class rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1Movement">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1hasDuration"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1Duration"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Movement is the act of moving a certain BodyPart towards a certain direction in a certain period of time (Duration).</rdfs:comment>
        <rdfs:label xml:lang="en">Movement</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1Pose -->

    <owl:Class rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1Pose">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/hdgi/gesture-interaction-pattern1timestamp"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Pose is repesent a particular Position and Rotation of a BodyPart at a particular timestamp.</rdfs:comment>
        <rdfs:label xml:lang="en">Pose</rdfs:label>
    </owl:Class>
</rdf:RDF>



