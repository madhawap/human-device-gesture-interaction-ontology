    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1timestamp -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1timestamp">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <rdfs:comment xml:lang="en">A timestamp represent the date and time of an event occured and that is recorded by a system/person.</rdfs:comment>
        <rdfs:label xml:lang="en">timestamp</rdfs:label>
    </owl:DatatypeProperty>
    


