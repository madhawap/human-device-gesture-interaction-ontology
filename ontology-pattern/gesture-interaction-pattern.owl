    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1affordedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1affordedBy">
        <rdfs:comment xml:lang="en">Relation between an Affordance and the Device classes where a device is able to afford it.</rdfs:comment>
        <rdfs:label xml:lang="en">affordedBy</rdfs:label>
        <skos1:example xml:lang="en">Turn on TV</skos1:example>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1hasDuration -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1hasDuration">
        <rdfs:comment xml:lang="en">Relation between a Gesture and the Duration, where the gesture has a duration. This is an owl:equivalentClass time:hasDuration.</rdfs:comment>
        <rdfs:label xml:lang="en">hasDuration</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1hasEndPose -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1hasEndPose">
        <rdfs:comment xml:lang="en">Relation between a Gesture and a Pose where the pose represents the ending Pose of that gesture.</rdfs:comment>
        <rdfs:label xml:lang="en">hasEndPose</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1hasMovement -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1hasMovement">
        <rdfs:comment xml:lang="en">Relation between a Gesture and a Movement where the movement describes a particular motion of a body part.</rdfs:comment>
        <rdfs:label xml:lang="en">hasMovement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1hasStartPose -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1hasStartPose">
        <rdfs:comment xml:lang="en">Relation between a Gesture and Pose where the pose represents the initial pose of that gesture.</rdfs:comment>
        <rdfs:label xml:lang="en">hasStartPose</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1includesGesture -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1includesGesture">
        <rdfs:comment xml:lang="en">A Gesture could consist multiple atomic Gestures. This represents the relation between the Gesture and its atomic Gestures.</rdfs:comment>
        <rdfs:label xml:lang="en">includesGestures</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1performs -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1performs">
        <rdfs:comment xml:lang="en">Relation between a Human and a Gesture.</rdfs:comment>
        <rdfs:label xml:lang="en">performs</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1supportsGesture -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1supportsGesture">
        <rdfs:comment xml:lang="en">Relation between a Gesture and an Affordance where affordance is supported by that particular gesture.</rdfs:comment>
        <rdfs:label xml:lang="en">supportsGesture</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1used -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1used">
        <rdfs:comment xml:lang="en">Relation between a Gesture and a BodyPart which describes the body parts used in that gesture. This is an owl:equivalentClass prov:used.</rdfs:comment>
        <rdfs:label xml:lang="en">used</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/hdgi/gesture-interaction-pattern1uses -->

    <owl:ObjectProperty rdf:about="https://w3id.org/hdgi/gesture-interaction-pattern1uses">
        <rdfs:comment xml:lang="en">Relation between a Human and BodyPart which describes the body part that a human uses to perform a certain gesture.</rdfs:comment>
        <rdfs:label xml:lang="en">uses</rdfs:label>
    </owl:ObjectProperty>
    


